pipeline { 
    agent any 
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') { 
            steps { 
                echo('Build Backend')
                msBuild {
                    msBuildInstallation('Default')
                    buildFile('api/TrackBEE-API.csproj')
                    args('check')
                    args('another')
                    passBuildVariables()
                    continueOnBuildFailure()
                    unstableIfWarnings()
                }
            }
        }
        stage('Test'){
            steps {
                echo('Test')
            }
        }
        stage('Deploy') {
            steps {
                echo('Deploy')
            }
        }
    }
}
